// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Client {
  id         String   @id @default(uuid())
  name       String
  created_at DateTime

  @@map("clients")
}

model Adress {
  id         String @id @default(uuid())
  street     String
  number     Int
  complement String
  district   String

  @@map("adresses")
}
